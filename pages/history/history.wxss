/* history.wxss */
.record-card {
  background: white;
  margin: 20rpx;
  padding: 30rpx;
  border-radius: 15rpx;
  box-shadow: 0 2rpx 6rpx rgba(0,0,0,0.1);
}

.header {
  display: flex;
  justify-content: space-between;
  align-items: center;
  border-bottom: 1rpx solid #eee;
  padding-bottom: 20rpx;
}

.content {
  margin: 20rpx 0;
  display: flex;
  justify-content: space-between;
}

.note {
  color: #666;
  font-size: 28rpx;
}